{
  "name": "enrolla-quiz-monorepo",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "workspaces": [
    "quiz-app",
    "server",
    "shared"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format",
    "test": "pnpm -r --workspace-concurrency=1 test"
  }
}
